                                    <div class="circle3"></div>
                                    <div class="circle4"></div>
                                </div>
                            </div>

                            <div data-min-width="381" class="isotope_main animation">
                                <!-- ket-chup -->
                                @{ int delay = 1; }
                                @for (int i = 1; i <= ViewBag.Images_Paladin.ket_chup; i++)
                                {
                                    <div class="isotope_item ket-chup">
                                        <div class="photo animation_item scaleUp delay@((delay == 10 ? delay = 1 : delay++))">
                                            <a href="#">
                                                <img alt="" src="@Url.Content("~/Resources/images/png/palandin/ket-chup/" + i + ".png")" />
                                            </a>
                                            <span class="angle"></span>
                                        </div>
                                    </div>
                                }
                                <!-- ./ket-chup -->
                                <!-- mayonaise -->
                                @for (int i = 1; i <= ViewBag.Images_Paladin.mayonaise; i++)
