<!-- Quinas -->
<div id="dnn_Full_Screen_PaneH" class="Full_Screen_PaneH">
    <div class="DnnModule DnnModule-DNN_HTML DnnModule-28351">
        <a name="28351"></a>
        <div class="White">
            <div id="dnn_ctr28351_ContentPane" class="contentpane">
                <!-- Start_Module_28351 -->
                <div id="dnn_ctr28351_ModuleContent" class="DNNModuleContent ModDNNHTMLC">
                    <div id="dnn_ctr28351_HtmlModule_lblContent" class="Normal">
                        <div class="backgroundImage9">
                            <div class="dnn_layout pt-60 animation scaleUp">
                                <div class="content_mid clearfix">
                                    <h3 class="title_style_3 pb-20" style="color: white;">
                                        QUINAS
                                    </h3>
                                    <p>Pour les amateurs de l'art de boire une bonne bière</p>
                                </div>
                            </div>

                            <div class="dnn_layout pb-30">

                                <div class="row">
                                    @for (int i = 1; i <= ViewBag.Images_Quinas; i++)
                                    {
                                        <div class="col-xs-6 col-lg-3">
                                            <div class="animation_item zoomInLeft delay@(i) pb-30">
                                                <img alt="Images BOPACK" src="@Url.Content("~/Resources/images/png/quinas/" + i + ".png")" class="img-responsive" />
                                                <div>
                                                    @if (i == 1)
                                                    {
                                                        <p class="lead" style="font-weight: bold;">La Mini</p>
                                                        <i>25 Cl</i><br />
                                                        <p>La bière qui descend glaciale, pour les gorgées les plus rafraichissantes!</p>
                                                    }
                                                    else if (i == 2)
                                                    {
                                                        <p class="lead" style="font-weight: bold;">L'original</p>
                                                        <i>33 Cl</i><br />
                                                        <p>Pour ceux qui aiment prolonger le bon goût d'une bière très froide!</p>
                                                    }
                                                    else if (i == 3)
                                                    {
                                                        <p class="lead" style="font-weight: bold;">Aux villes</p>
                                                        <i>50 Cl</i><br />
                                                        <p>Pour les amateurs de bières différentes et uniques!</p>
                                                    }
                                                    else
                                                    {
                                                        <p class="lead" style="font-weight: bold;">La première édition</p>
                                                        <i>75 Cl</i><br />
                                                        <p>Une bière forte et robuste avec une identité propre!</p>
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                <!-- End_Module_28351 -->
            </div>
        </div>
    </div>
</div>
<!-- ./Quinas -->

