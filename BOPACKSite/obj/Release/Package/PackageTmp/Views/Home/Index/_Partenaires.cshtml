                                <a data-filter=".mayonaise" href="#">MAYONNAISE</a>
                                <a data-filter=".moutarde" href="#">MOUTARDE</a>
                                <a data-filter=".restaurant" href="#">RESTAURANT</a>
                                <a data-filter=".sauce" href="#">SAUCE</a>
                                <a data-filter=".sauce-piquante" href="#">SAUCE PIQUANTE</a>
                                <a data-filter=".vinaigre" href="#">VINAIGRE</a>
                            </div>

                            <div class="loading  animation scaleUp">
