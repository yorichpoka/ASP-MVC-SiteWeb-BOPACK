@model dynamic

<!-- Meta data -->
<meta lang="fr" name="Keyword"      content="@Model.Mot_Cles_FR" />
<!-- Open Graph Property -->
<meta property="og:title"           content="@Model.OG_TITLE" />
<meta property="og:type"            content="@Model.OG_TYPE" />
<meta property="og:url"             content="@Model.OG_URL" />
<meta property="og:image"           content="@Model.OG_IMAGE" />
<meta property="og:description"     content="@Model.OG_DESCRIPTION" />
<!-- Twitter Card data -->
<meta name="twitter:title"          content="@Model.OG_TITLE" />
<meta name="twitter:card"           content="summary" />
<meta name="twitter:site"           content="@Model.OG_URL" />
<meta name="twitter:description"    content="@Model.OG_DESCRIPTION" />
<meta name="twitter:creator"        content="Ulrich POKA" />
<!-- Twitter Summary card images must be at least 200x200px -->
<meta name="twitter:image"          content="@Model.OG_IMAGE" />
<!-- ./Open Graph Property -->
<!-- ./Meta data -->